								.start__btn-icon
				.start__item.js-motion(data-motion='motion1' data-main-delay='0' data-move-up='70' data-speed-line='1')
					.start__item-desc
						.start__title
							.start__title-text
								p профилактика
							.start__title-icon.start__title-icon_black
