								.start__btn-icon
				.start__item.js-motion(data-motion='motion3' data-main-delay='0.2' data-move-up='50' data-move-up-speed='1')
					img(src='img/content/start-6.jpg' alt='image')
					.start__item-desc
						.start__title
							.start__title-text
								p детский кабинет
							.start__title-icon.start__title-icon_black
						.start__hide
							.start__text
